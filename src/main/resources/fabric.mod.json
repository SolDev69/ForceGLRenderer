{
  "schemaVersion": 1,
  "id": "forceopengl",
  "version": "${version}",

  "name": "ForceGLRenderer",
  "description": "Not to be confused with ForceGL20. This mod tricks Minecraft into thinking you're running a newer version of OpenGL then you really have.",
  "authors": [
    "SolDev69"
  ],
  "contact": {
    "sources": "https://github.com/SolDev69/ForceGLRenderer"
  },
  "license": "CC0-1.0",
  "icon": "assets/forceopengl/icon.png",
  "environment": "client",
  "entrypoints": {
    "preLaunch": [
      "net.fabricmc.example.ExampleMod"
    ]
  },
  "depends": {
    "fabricloader": ">=0.14.8",
    "minecraft": ">=1.17-alpha.21.10.a",
    "java": ">=8"
  }
}
